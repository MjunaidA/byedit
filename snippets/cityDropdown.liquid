        }
        else if($(this).val() == 'DU'){
          $('[data-region="Dubai"]').show()
        }
        else if($(this).val() == 'FU'){
          $('[data-region="Fujairah"]').show()
        }
        else if($(this).val() == 'RK'){
          $('[data-region="Ras al-Khaimah"]').show()
        }
        else if($(this).val() == 'SH'){
          $('[data-region="Sharjah"]').show()
        }
        else if($(this).val() == 'UQ'){
          $('[data-region="Umm al-Quwain"]').show()
        }
      })

      $(cityField).click(function() {
        $('.cityDropdownCustom').show();
      });
      
      $(document).on('click','[data-city-item]',  function() {
        $('.cityDropdownCustom').hide(); 
        cityField.val($(this).attr('data-city-name'));
      })
      
      // Close city dropdown click outside the dropdown
      $(document).mouseup(e => {
      if (!cityFiledWrap.is(e.target) // if the target of the click isn't the container...
      && cityFiledWrap.has(e.target).length === 0) // ... nor a descendant of the container
      {
          $('.cityDropdownCustom').hide();
      }
      });

      // Search Functionality in City Dropdown
      $('.cityDropdownCustom .cityDropdownFieldWrap').on('keyup','#cityDropdownField', function() {
      var value = $(this).val().toUpperCase();
      var listItems = $('.cityDropdownCustom ul li');
      $(listItems).each(function() {
          if ($(this).text().toUpperCase().search(value) > -1) {
              $(this).show();
          }
          else {
              $(this).hide();
          }
      });
      }); 
    
        
    var arraCities = [];

    $('.cityDropdownCustom ul li').each(function() {
      arraCities.push($(this).attr('data-city-name'));
    });

    {% comment %} Only english language accepted for all text fields {% endcomment %}
    function validateInput(input) {
      var input = input.trim();
      var pattern = /^[A-Za-z0-9\s,.']+$/;

      if (!pattern.test(input)) {
        return false;
      }
      
      return true;
    }
    
    $('[data-step="contact_information"] > form').submit( function() {

      if($('.error-span').length){
        $('.error-span').remove()
      }
      var error = "<span class='error-span' style='color:red'>Only english language accepted. Do not use special tags</span>";
      {% comment %} validating first name {% endcomment %}
      var firstName = $('input#checkout_shipping_address_first_name').val()
      if(firstName != ""){
        if(!validateInput(firstName)){
          $("input#checkout_shipping_address_first_name").after(error)
          setTimeout(function() {
            $('.error-span').remove()
          }, 2500)
        return false
      }
      }

      {% comment %} validating last name {% endcomment %}
      var lastName = $('input#checkout_shipping_address_last_name').val()
      if(lastName != ""){
        if(!validateInput(lastName)){
          $("input#checkout_shipping_address_last_name").after(error)
          setTimeout(function() {
            $('.error-span').remove()
          }, 2500)
        return false
      }
      }

      {% comment %} validating Address 1 {% endcomment %}
      var address1 = $('input#checkout_shipping_address_address1').val()
      if(address1 != ""){
        if(!validateInput(address1)){
          $("input#checkout_shipping_address_address1").after(error)
          setTimeout(function() {
            $('.error-span').remove()
          }, 2500)
        return false
      }
      }

      {% comment %} validating Address 2 {% endcomment %}
      var address2 = $('input#checkout_shipping_address_address2').val()
      if(address2 != ""){
        if(!validateInput(address2)){
          $("input#checkout_shipping_address_address2").after(error)
          setTimeout(function() {
            $('.error-span').remove()
          }, 2500)
        return false
      }
      }

        var cityInput = $('input#checkout_shipping_address_city').val();
        if ($.inArray(cityInput, arraCities) > -1)
        {
          console.log('find');
          return true;
        }
        else
        {
          var a = "<span class='error-span' style='color:red'> Enter a valid city from city dropdown </span>";
          $("#checkout_shipping_address_city").after(a);
          setTimeout(function() {
            $('.error-span').remove()
          }, 2500)
          return false;
        }
    });
  });
</script>

